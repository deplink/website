{% extends "docs/_base-index.njk" %}
{% set docsPages = guidePages %}

{% macro printItemList(article) %}
    <b><a href="{{ url(article.url) }}">{{ article.menuTitle|default(article.title) }}</a></b>
    {% if article.description %}
        <div class="font-tiny text-gray">{{ article.description }}</div>
    {% endif %}
{% endmacro %}